{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "es6",
        "outDir": "dist",
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "declaration": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": false,
        "types": ["cypress", "node"],
        "moduleResolution": "bundler",
        "module": "es6"
    },
    "include": ["src"]
}
